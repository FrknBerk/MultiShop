@model CreateFeatureDto
@{
    ViewData["Title"] = "CreateFeature";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<section class="content container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="chart-box">
                <div class="bs-example" data-example-id="bordered-table">
                    <form method="post">
                        <input type="text" class="form-control" asp-for="Title" placeholder="Öne Çıkan Alan Başlığı Giriniz..." />
                        <br />
                        <input type="text" class="form-control" asp-for="Icon" placeholder="İkon Giriniz..." />
                        <br />
                        <button class="btn btn-primary">Öne Çıkan Alan Ekle</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
